{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "out",
		"lib": ["ES2020", "DOM"],
		"rootDir": "src",
		"sourceMap": true,

		"strict": true,
		"strictNullChecks": false,
		//MO DEV
		// "noUnusedLocals": true,
		// "noUnusedParameters": true,
		"removeComments": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true
	}
}
